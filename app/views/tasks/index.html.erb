<% @tasks.each do |task| %>
  <%= link_to "Task: #{task.name} Due: #{task.due_date.strftime("%d %B")} Status: #{task.status} Description: #{task.description}, Users IDS Assigned: #{task.user_ids.each { |id| User.find(id).first_name }}", edit_task_path(task) %>
    <% @users = task.user_ids %>
    <% @users.each do |id| %>
      <% User.find(id).first_name %>
    <% end %>
   <br>
<% end %>
